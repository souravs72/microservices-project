# Staging Environment Variables
# Copy this file to .env.staging and update the values

# Database Configuration
DB_PASSWORD=staging_db_password_example

# Redis Configuration
REDIS_PASSWORD=staging_redis_password_example

# JWT Configuration
JWT_SECRET=staging_jwt_secret_example_change_me

# Mail Configuration
MAIL_USERNAME=your_email@example.com
MAIL_PASSWORD=your_mail_app_password_example

# Internal API Keys
INTERNAL_API_KEY=staging_internal_api_key_example

# Admin Configuration
ADMIN_USERNAME=admin
ADMIN_PASSWORD=staging_admin_password_example

# CORS Configuration
CORS_ALLOWED_ORIGINS=https://staging.yourdomain.com,http://localhost:3000,http://localhost:5173

# Monitoring
GRAFANA_PASSWORD=staging_grafana_password_example
